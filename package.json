{
    "name": "dot-portal",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "run-s build:webpack build:jekyll",
        "build:jekyll": "JEKYLL_ENV=production NODE_ENV=production bundle exec jekyll build --trace",
        "build:webpack": "NODE_ENV=production webpack --mode production",
        "serve": "run-p serve:{webpack,jekyll}",
        "serve:jekyll": "sleep 5 && bundle exec jekyll serve --livereload",
        "serve:webpack": "webpack --mode development --watch",
        "prepare": "husky install",
        "deploy": "npm run build && gh-pages -d docs",
        "minify-css": "JEKYLL_ENV=production NODE_ENV=production postcss -o docs/assets/css/styles.css docs/assets/css/styles.css",
        "prettier:docs": "npx pretty-quick docs"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/preset-env": "^7.16.11",
        "@fullhuman/postcss-purgecss": "^4.0.3",
        "autoprefixer": "^10.3.4",
        "babel-loader": "^8.2.3",
        "copy-webpack-plugin": "^6.4.1",
        "css-loader": "^5.2.7",
        "cssnano": "^5.0.17",
        "file-loader": "^6.2.0",
        "gh-pages": "^3.2.3",
        "husky": "^7.0.0",
        "mini-css-extract-plugin": "^1.6.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.3.9",
        "postcss-cli": "^9.0.1",
        "postcss-loader": "^4.3.0",
        "postcss-preset-env": "^7.4.1",
        "postcss-scss": "^4.0.1",
        "prettier": "2.4.1",
        "pretty-quick": "^3.1.1",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.49.8",
        "sass-loader": "^10.2.1",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^4.2.3",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.2",
        "@uppy/core": "^2.1.5",
        "@uppy/form": "^2.0.4",
        "@uppy/xhr-upload": "^2.0.7",
        "bootstrap": "^5.1.1",
        "core-js": "^3.21.1",
        "uuid": "^8.3.2"
    }
}
